{
  "name": "regrapdf",
  "version": "1.0.0",
  "description": "GraphQL powered PDF Generation",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "build": "babel ./src -d ./dist",
    "dev": "nodemon --exec babel-node src/server.js",
    "start": "node ./dist/server.js",
    "test": "jest --watch",
    "test:ci": "start-server-and-test start tcp:4000 jest"
  },
  "author": "Christoph Benjamin Weber",
  "license": "MIT",
  "dependencies": {
    "@react-pdf/renderer": "1.6.7",
    "apollo-server": "2.9.7",
    "graphql": "14.5.8",
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/node": "7.6.3",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "babel-jest": "24.9.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "24.9.0",
    "nodemon": "1.19.4",
    "prettier": "1.18.2",
    "start-server-and-test": "1.10.6"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
